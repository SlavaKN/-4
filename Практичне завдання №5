n = int(input("Введіть довжину послідовності n: "))
MOD = 12345

# dp[i] — кількість правильних послідовностей довжини i
dp = [0] * (n + 3)
dp[0] = 1
dp[1] = 2
dp[2] = 4

for i in range(3, n + 1):
    dp[i] = (dp[i - 1] + dp[i - 2] + dp[i - 3]) % MOD

print(f"Кількість шуканих послідовностей: {dp[n]}")
